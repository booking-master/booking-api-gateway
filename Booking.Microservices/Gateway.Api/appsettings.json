{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Issuer": "Booking",
    "Audience": "Booking",
    "SecretKey": "my_awesome_secret_key_the_best_in_world"
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "modular-route": {
        "ClusterId": "modular-cluster",
        "Match": {
          "Path": "modular/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/modular"
          }
        ]
      }
    },
    "Clusters": {
      "modular-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7096/"
          }
        }
      }
    }
  }
}
